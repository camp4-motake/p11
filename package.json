{
  "name": "p11",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "concurrently 'npx @11ty/eleventy --watch --incremental --quiet' 'parcel \"./.tmp/**/*.html\" --dist-dir dist'",
    "build": "tsc --noEmit && npx @11ty/eleventy && parcel build \"./.tmp/**/*.html\" --dist-dir dist/ --no-cache --no-source-maps",
    "format": "prettier --write --list-different . && stylelint 'src/**/*.css' --fix && eslint '**/*.{js,mjs,cjs,ts}' --fix",
    "clean": "rimraf dist/* .tmp/*",
    "predev": "npm run clean",
    "prebuild": "npm run clean",
    "//postbuild": "node lib/relativePath.js"
  },
  "dependencies": {
    "@alpinejs/intersect": "^3.14.1",
    "alpinejs": "^3.14.1",
    "ress": "^5.0.2"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@parcel/resolver-glob": "^2.12.0",
    "@types/alpinejs": "^3.13.10",
    "@types/node": "^20.14.12",
    "concurrently": "^8.2.2",
    "image-size": "^1.1.1",
    "parcel": "^2.12.0",
    "parcel-namer-rewrite": "^2.10.3-rc.2",
    "postcss": "^8.4.40",
    "postcss-easy-import": "^4.0.0",
    "rimraf": "^6.0.1",
    "sharp": "^0.31.3"
  },
  "peerDependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/js": "^9.8.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.8.0",
    "prettier": "^3.3.3",
    "prettier-plugin-jinja-template": "^1.4.1",
    "stylelint": "16.7.0",
    "stylelint-config-recess-order": "5.0.1",
    "stylelint-config-recommended": "14.0.1",
    "typescript-eslint": "^8.0.0-alpha.10"
  },
  "@parcel/transformer-css": {
    "drafts": {
      "nesting": true,
      "customMedia": true
    }
  },
  "parcel-namer-rewrite": {
    "chain": "@parcel/namer-default",
    "hashing": "always",
    "disable": false,
    "silent": true,
    "rules": {
      "(.*).(css|js|webp|avif|png|jpe?g|gif|svg|mp4|json)": "assets/$1{.hash}.$2"
    }
  },
  "browserslist": [
    "> 0.5% in JP and last 2 years and not dead"
  ],
  "volta": {
    "node": "20.16.0"
  }
}
